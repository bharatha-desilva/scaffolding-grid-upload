{
    "name": "bourque.gridupload.app",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "prestart": "node aspnetcore-https",
        "start": "run-script-os",
        "start:windows": "ng serve --port 44410 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
        "start:default": "ng serve --port 44410 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
        "build": "ng build",
        "build:ssr": "ng run Bourque.GridUpload.App:server:dev",
        "watch": "ng build --watch --configuration development",
        "test": "jest --coverage",
        "lint": "ng lint",
        "prepare": "is-ci || cd .. && cd .. && husky install Bourque.GridUpload.App/ClientApp/.husky"
    },
    "private": true,
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "dependencies": {
        "@angular/animations": "~12.2.2",
        "@angular/cdk": "~12.2.10",
        "@angular/common": "~12.2.2",
        "@angular/compiler": "~12.2.2",
        "@angular/core": "~12.2.2",
        "@angular/forms": "~12.2.2",
        "@angular/material": "~12.2.10",
        "@angular/platform-browser": "~12.2.2",
        "@angular/platform-browser-dynamic": "~12.2.2",
        "@angular/platform-server": "~12.2.2",
        "@angular/router": "~12.2.2",
        "@bds/applications": "^1.0.4-alpha.1",
        "@bds/auth": "^1.1.4-alpha.1",
        "@bds/core": "^1.0.4-alpha.1",
        "@bds/fa-svg-icons": "^1.0.0",
        "@bds/helpers": "^1.0.4-alpha.1",
        "@bds/models": "^1.0.4-alpha.1",
        "@bds/services": "^1.0.4-alpha.1",
        "@bds/ui": "^1.0.4-alpha.1",
        "@fortawesome/angular-fontawesome": "^0.9.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-brands-svg-icons": "^5.15.3",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "devextreme": "~21.1",
        "moment": "^2.24.0",
        "bootstrap": "^5.1.0",
        "jquery": "^3.5.1",
        "oidc-client": "^1.11.5",
        "popper.js": "^1.16.0",
        "run-script-os": "^1.1.6",
        "rxjs": "~6.6.0",
        "tslib": "^2.1.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.2.2",
        "@angular-eslint/builder": "12.5.0",
        "@angular-eslint/eslint-plugin": "12.5.0",
        "@angular-eslint/eslint-plugin-template": "12.5.0",
        "@angular-eslint/schematics": "12.5.0",
        "@angular-eslint/template-parser": "12.5.0",
        "@angular/cli": "~12.2.2",
        "@angular/compiler-cli": "~12.2.2",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "^12.11.1",
        "jasmine-core": "~3.8.0",
        "@types/jest": "^27.0.2",
        "@typescript-eslint/eslint-plugin": "4.28.2",
        "@typescript-eslint/parser": "4.28.2",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^27.3.1",
